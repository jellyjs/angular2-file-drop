{
  "name": "bergben-angular2-file-drop",
  "version": "0.2.0",
  "description": "Angular2 component with Drag and Drop support for files",
  "main": "dist/bundles/bergben-angular2-file-drop.umd.js",
  "module": "dist/file-drop.js",
  "typings": "dist/file-drop.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/bergben-angular2-file-drop.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/bergben-angular2-file-drop.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jellyjs/angular2-file-drop.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "file",
    "drag",
    "drop",
    "upload"
  ],
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jellyjs/angular2-file-drop/issues"
  },
  "homepage": "https://github.com/jellyjs/angular2-file-drop#readme",
  "dependencies": {
    "fileapi": "^2.0.20"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/compiler-cli": "^2.4.8",
    "@angular/core": "^2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rollup": "^0.41.4",
    "rxjs": "5.0.0-beta.12",
    "tslint": "^3.4.0",
    "typescript": "^2.0.3",
    "uglify-js": "^2.7.5",
    "zone.js": "^0.6.21"
  }
}
